<data>
    <template id="portal_my_home_inherit" inherit_id="portal.portal_my_home">
        <xpath expr="//div[contains(@class, 'o_portal_docs')]" position="before">
            <br/>
            <t t-call="portal.portal_docs_entry">
                
                <t t-set="icon" t-value="'/project/static/src/img/tasks.svg'"/>
                <t t-set="title">Historial Servicios</t>
                <t t-set="text">Solicite y vea sus mantenimientos</t>
                <t t-set="url">/my/sedes</t>
                <t t-set="config_card" t-value="True"/>
            </t>
            <br/>
            <t t-call="portal.portal_docs_entry">
                
                <t t-set="icon" t-value="'/project/static/src/img/tasks.svg'"/>
                <t t-set="title">Certificados de Operatividad</t>
                <t t-set="text">Obten informacion sobre los certificados de operatividad</t>
                <t t-set="url">/my/sedes</t>
                <t t-set="config_card" t-value="True"/>
            </t>
            <br/>
        </xpath>
    </template>
  </data>
  