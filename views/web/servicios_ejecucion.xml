				<div class="main-card-sedes">
					<div style="display:flex;flex-flow: row wrap;justify-content: space-between;gap: 0px;align-items: center;">
						<h3>
							Servicios en Ejecución
						</h3>
					</div>
					<div class="main-sedes">
						<t t-if="len(equipo.planequipo) > 0">
							<t t-foreach="equipo.planequipo" t-as="plan">
								<t t-if="plan.tarea.ots">
									<t t-foreach="plan.tarea.ots" t-as="ot">
										<t t-if="ot.stage_id.id in [1, 2]">
											<div class="card-mantenimiento">
												<div style="display:flex; gap:5px;align-items:center;">
													<img src="/pmant/static/src/css/iconos/mantenimiento.png" alt="Imagen de mantenimiento" />
													<div class="text-card-mantenimiento">
														<strong t-esc="plan.tarea.name" />
														<span>
															<span>
																Plan de Mant. :
															</span>
															<span t-esc="plan.plan.name" />
														</span>
														<span>
															<span>
																Fecha programada :
															</span>
																<span t-esc="ot.schedule_date" />
														</span>
														<span>
															<span>
																Etapa :
															</span>
															<span t-esc="plan.estado" />
														</span>
													</div>
												</div>
												<a t-att-href="'/descargas/reporte/mantenimiento/%d' % (plan.tarea.id)" class="bg-primary">
													<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-arrow-down" viewBox="0 0 16 16">
														<path d="M8.5 6.5a.5.5 0 0 0-1 0v3.793L6.354 9.146a.5.5 0 1 0-.708.708l2 2a.5.5 0 0 0 .708 0l2-2a.5.5 0 0 0-.708-.708L8.5 10.293z" />
														<path d="M14 14V4.5L9.5 0H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2M9.5 3A1.5 1.5 0 0 0 11 4.5h2V14a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h5.5z" />
													</svg>
												</a>
											</div>
										</t>
